Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "*", "Defining", " ", "function", " ", "to", " ", "calcualet", " ", 
    "inner", " ", "product", " ", "and", " ", "overlap"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"InnerProduct", "[", 
      RowBox[{
      "min_", ",", " ", "max_", ",", " ", "q_", ",", "n_", ",", "u1_", ",", 
       " ", "u2_"}], "]"}], ":=", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Wavefunction", "[", 
         RowBox[{"q", ",", " ", "n", ",", " ", "u1"}], "]"}], "*", 
        RowBox[{"Conjugate", "[", 
         RowBox[{"Wavefunction", "[", 
          RowBox[{"q", ",", "n", ",", "u2"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"z", ",", " ", "min", ",", " ", "max"}], "}"}], ",", 
       RowBox[{"PrecisionGoal", "\[Rule]", "8"}], ",", " ", 
       RowBox[{"WorkingPrecision", "\[Rule]", "16"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
