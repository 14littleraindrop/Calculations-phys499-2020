Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Defining", " ", "3", "D", " ", "wavefunction"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Wavefunction3D", "[", 
      RowBox[{"q_", ",", " ", "n_", ",", " ", "u_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"MathieuPotentialWaveFunction", "[", 
       RowBox[{
        RowBox[{"TranslateQuasimomentum", "[", 
         RowBox[{"q", ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"u", "/", "4"}], ",", " ", 
        SubscriptBox["z", "x"]}], "]"}], "*", 
      RowBox[{"MathieuPotentialWaveFunction", "[", 
       RowBox[{
        RowBox[{"TranslateQuasimomentum", "[", 
         RowBox[{"q", ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"u", "/", "4"}], ",", " ", 
        SubscriptBox["z", "y"]}], "]"}], "*", 
      RowBox[{"MathieuPotentialWaveFunction", "[", 
       RowBox[{
        RowBox[{"TranslateQuasimomentum", "[", 
         RowBox[{"q", ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"u", "/", "4"}], ",", " ", 
        SubscriptBox["z", "z"]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"InnerProduct3D", "[", 
      RowBox[{
      "min_", ",", " ", "max_", ",", " ", "q_", ",", "n_", ",", "u1_", ",", 
       " ", "u2_"}], "]"}], ":=", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Wavefunction3D", "[", 
         RowBox[{"q", ",", " ", "n", ",", " ", "u1"}], "]"}], "*", 
        RowBox[{"Conjugate", "[", 
         RowBox[{"Wavefunction3D", "[", 
          RowBox[{"q", ",", "n", ",", "u2"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{
         SubscriptBox["z", "x"], ",", " ", "min", ",", " ", "max"}], "}"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{
         SubscriptBox["z", "y"], ",", " ", "min", ",", " ", "max"}], "}"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{
         SubscriptBox["z", "z"], ",", " ", "min", ",", " ", "max"}], "}"}], 
       ",", 
       RowBox[{"PrecisionGoal", "\[Rule]", "8"}], ",", " ", 
       RowBox[{"WorkingPrecision", "\[Rule]", "16"}]}], "]"}]}], ";"}], 
