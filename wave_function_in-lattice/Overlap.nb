        RowBox[{"Conjugate", "[", 
         RowBox[{"Wavefunction", "[", 
          RowBox[{"q", ",", "n", ",", "u2"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"z", ",", " ", "min", ",", " ", "max"}], "}"}], ",", 
       RowBox[{"PrecisionGoal", "\[Rule]", "8"}], ",", " ", 
       RowBox[{"WorkingPrecision", "\[Rule]", "16"}]}], "]"}]}], ";"}], 
