Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "*", "Defining", " ", "function", " ", "to", " ", "calcualet", " ", 
    "inner", " ", "product", " ", "and", " ", "overlap"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
